{
  "name": "dspx",
  "version": "0.1.2-alpha.0",
  "description": "High-performance DSP library with native C++ acceleration and Redis state persistence",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node scripts/test.js",
    "semantic-release": "semantic-release",
    "build:ts": "tsc",
    "build": "npm run build:ts && node-gyp rebuild && node -e \"const fs = require('fs'); fs.cpSync('build/Release/dspx.node', 'src/build/dspx.node', {force: true}); console.log('Copied build/Release/dspx.node to src/build/dspx.node');\"",
    "prebuildify": "prebuildify --napi --strip --target 18.0.0 --target 20.0.0 --target 22.0.0"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "keywords": [
    "dsp",
    "signal-processing",
    "native",
    "redis",
    "real-time",
    "audio",
    "biosignal"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "jest": "^30.2.0",
    "prebuildify": "^6.0.1",
    "redis": "^5.9.0",
    "semantic-release": "^25.0.1",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
