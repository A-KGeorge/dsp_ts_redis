{
  "name": "dspx",
  "version": "0.2.0-alpha.0",
  "description": "High-performance DSP library with native C++ acceleration and Redis state persistence",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/build/dspx.node",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "node scripts/test.js",
    "semantic-release": "semantic-release",
    "build:ts": "tsc",
    "build:native": "node-gyp rebuild && node -e \"const fs = require('fs'); const path = require('path'); fs.mkdirSync('src/build', { recursive: true }); fs.mkdirSync('dist/build', { recursive: true }); fs.cpSync('build/Release/dspx.node', 'src/build/dspx.node', { force: true }); fs.cpSync('build/Release/dspx.node', 'dist/build/dspx.node', { force: true }); console.log('âœ… Copied native module to src/build and dist/build');\"",
    "build": "npm run build:native && npm run build:ts",
    "prebuildify": "prebuildify --napi --strip --target 18.0.0 --target 20.0.0 --target 22.0.0"
  },
  "release": {
    "branches": [
      {
        "name": "main",
        "channel": "alpha",
        "prerelease": "alpha"
      }
    ],
    "repositoryUrl": "https://github.com/A-KGeorge/dspx.git"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "keywords": [
    "dsp",
    "signal-processing",
    "native",
    "redis",
    "real-time",
    "audio",
    "biosignal"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "cross-env": "^7.0.3",
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "jest": "^30.2.0",
    "prebuildify": "^6.0.1",
    "redis": "^5.9.0",
    "semantic-release": "^25.0.1",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
