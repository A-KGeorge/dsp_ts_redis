{
  "name": "dsp-js-native",
  "version": "1.0.0",
  "description": "High-performance DSP library with native C++ acceleration and Redis state persistence",
  "main": "index.js",
  "scripts": {
    "test": "node --import tsx --test src/ts/__tests__/MovingAverage.test.ts src/ts/__tests__/Rectify.test.ts src/ts/__tests__/RMS.test.ts src/ts/__tests__/Chaining.test.ts src/ts/__tests__/Redis.test.ts",
    "test:watch": "node --import tsx --test --watch src/ts/__tests__/**/*.test.ts",
    "build": "cmake-js compile",
    "build-gyp": "node-gyp rebuild",
    "prebuild": "prebuildify --napi --strip",
    "start": "tsx ./src/ts/index.ts"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "keywords": [
    "dsp",
    "signal-processing",
    "native",
    "redis",
    "real-time",
    "audio",
    "biosignal"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.0",
    "redis": "^5.8.3"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "cmake-js": "^7.3.1",
    "jest": "^30.2.0",
    "prebuildify": "^6.0.1",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6"
  }
}
